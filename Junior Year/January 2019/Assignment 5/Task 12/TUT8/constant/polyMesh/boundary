    {
        type            patch;
        nFaces          20;
        startFace       8180;
    }
    outlet
    {
        type            patch;
        nFaces          20;
        startFace       8200;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          8000;
